{
  "name": "chrome-ext-ntp-bookmarkers",
  "description": "Chrome extension new tab bookmarkers",
  "scripts": {
    "lint": "eslint src tools --fix",
    "prebuild": "npm run lint && rm -fr dist && mkdir dist && cp -R src/{icons,manifest.json} dist",
    "build": "parcel build src/ntp.html src/background.js --no-cache --no-source-maps --out-dir dist/",
    "dev": "npm run prebuild && parcel watch src/ntp.html src/background.js --hmr-hostname localhost --hmr-port 8081 --no-cache --no-source-maps --out-dir dist/",
    "pack": "npm run build && node tools/pack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertoentringer/chrome-ext-ntp-bookmarkers.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "new tab page",
    "ntp"
  ],
  "author": "Roberto Entringer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robertoentringer/chrome-ext-ntp-bookmarkers/issues"
  },
  "homepage": "https://github.com/robertoentringer/chrome-ext-ntp-bookmarkers#readme",
  "devDependencies": {
    "archiver": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-prettier": "^3.1.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.1"
  },
  "dependencies": {
    "sortablejs": "^1.9.0"
  }
}
